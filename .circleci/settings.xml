<settings>
    <servers>
        <server>
            <id>sonatype-nexus-snapshots</id>
            <username>${env.CI_DEPLOY_USERNAME}</username>
            <password>${env.CI_DEPLOY_PASSWORD}</password>
        </server>
    </servers>
    <activeProfiles>
        <activeProfile>jdkpaths</activeProfile>
    </activeProfiles>
    <profiles>
        <profile>
            <id>jdkpaths</id>
            <properties>
                <jdk.8.home>/usr/lib/jvm/java-8-openjdk-amd64</jdk.8.home>
                <jdk.11.home>/usr/lib/jvm/java-11-openjdk-amd64</jdk.11.home>
                <jdk.13.home>/usr/java/openjdk-13</jdk.13.home>
            </properties>
        </profile>
        <profile>
            <id>skip</id>
            <properties>
                <maven.resources.skip>true</maven.resources.skip>
                <maven.main.skip>true</maven.main.skip>
                <maven.javadoc.skip>true</maven.javadoc.skip>
            </properties>
        </profile>
    </profiles>
</settings>
