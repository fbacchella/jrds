<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN"
	"urn:jrds:probedesc">
<probedesc>
	<name>name</name>
	<probeName>probename</probeName>
	<probeClass>jrds.probe.HttpXml</probeClass>
	<specific name="upTimePath">/element/@uptime</specific>
	<uptimefactor>0.5</uptimefactor>
	<defaultargs>
        <attr name="port">80</attr>
	</defaultargs>
	<ds>
		<dsName>a</dsName>
		<dsType>counter</dsType>
		<collect>/jrdsstats/stat[@key='%1$s']/@value</collect>
	</ds>
	<ds>
		<dsName>b</dsName>
		<dsType>counter</dsType>
		<collect>/jrdsstats/stat[@key='b']/@value</collect>
	</ds>
	<ds>
		<dsName>c</dsName>
		<dsType>counter</dsType>
		
	</ds>
	<ds>
		<dsName>d</dsName>
		<dsType>counter</dsType>
		<collect>%2$s</collect>
	</ds>
	<graphs>
	</graphs>
    <archive>
        <consolFun>AVERAGE</consolFun>
        <xff>0.5</xff>
        <steps>2</steps>
        <rows>3000</rows>
    </archive>

    <archive>
        <consolFun>MIN</consolFun>
        <xff>0.7</xff>
        <steps>10</steps>
        <rows>5000</rows>
    </archive>

</probedesc>