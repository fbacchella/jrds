<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
  <name>MysqlTableSpace2</name>
  <probeName>mysqltablespace-${attr.table}</probeName>
  <probeClass>jrds.probe.jdbc.MysqlTableSpace2</probeClass>
  <specific name="query">show table status from ${attr.table};</specific>
  <ds>
    <dsName>Data_length</dsName>
    <dsType>gauge</dsType>
  </ds>
  <ds>
    <dsName>Index_length</dsName>
    <dsType>gauge</dsType>
  </ds>
  <ds>
    <dsName>Data_free</dsName>
    <dsType>gauge</dsType>
  </ds>
  <graphs>
    <name>MysqlTableSpace2</name>
  </graphs>
</probedesc>
