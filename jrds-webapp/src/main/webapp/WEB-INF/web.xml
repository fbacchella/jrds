<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>jrds</display-name>
    <listener>
        <listener-class>jrds.webapp.StartListener</listener-class>
    </listener>
    <listener>
        <listener-class>org.apache.commons.fileupload2.jakarta.servlet5.JakartaFileCleaner</listener-class>
    </listener>
    <servlet>
        <servlet-name>CheckValues</servlet-name>
        <servlet-class>jrds.webapp.CheckValues</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Graph</servlet-name>
        <servlet-class>jrds.webapp.Graph</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>download</servlet-name>
        <servlet-class>jrds.webapp.Download</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reload</servlet-name>
        <servlet-class>jrds.webapp.ReloadHostList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>probesummary</servlet-name>
        <servlet-class>jrds.webapp.ProbeSummary</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>probedetails</servlet-name>
        <servlet-class>jrds.webapp.JSonDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>whichlibs</servlet-name>
        <servlet-class>jrds.webapp.WhichLibs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>status</servlet-name>
        <servlet-class>jrds.webapp.Status</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>discover</servlet-name>
        <display-name>discover</display-name>
        <description></description>
        <servlet-class>jrds.webapp.Discover</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JSonTree</servlet-name>
        <display-name>JSonTree</display-name>
        <description></description>
        <servlet-class>jrds.webapp.JSonTree</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JSonGraph</servlet-name>
        <display-name>JSonGraph</display-name>
        <description></description>
        <servlet-class>
            jrds.webapp.JSonGraph</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JSonPeriod</servlet-name>
        <display-name>JSonPeriod</display-name>
        <description></description>
        <servlet-class>jrds.webapp.JSonPeriod</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JSonQueryParams</servlet-name>
        <display-name>JSonQueryParams</display-name>
        <description></description>
        <servlet-class>
            jrds.webapp.JSonQueryParams</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cmd</servlet-name>
        <display-name>Cmd</display-name>
        <description></description>
        <servlet-class>
            jrds.webapp.Cmd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JSonPack</servlet-name>
        <display-name>JSonPack</display-name>
        <description></description>
        <servlet-class>
            jrds.webapp.JSonPack</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>upload</servlet-name>
        <display-name>upload</display-name>
        <description></description>
        <servlet-class>
            jrds.webapp.Upload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetDiscoverHtmlCode</servlet-name>
        <display-name>GetDiscoverHtmlCode</display-name>
        <description></description>
        <servlet-class>jrds.webapp.GetDiscoverHtmlCode</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WhoAmI</servlet-name>
        <display-name>WhoAmI</display-name>
        <description></description>
        <servlet-class>jrds.webapp.WhoAmI</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetGraphDesc</servlet-name>
        <display-name>GetGraphDesc</display-name>
        <description>Return the graphdesc from the given graph Id</description>
        <servlet-class>jrds.webapp.GetGraphDesc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckValues</servlet-name>
        <url-pattern>/values/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Graph</servlet-name>
        <url-pattern>/graph</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Graph</servlet-name>
        <url-pattern>/graph/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>download</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>download</servlet-name>
        <url-pattern>/download/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reload</servlet-name>
        <url-pattern>/reload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>probesummary</servlet-name>
        <url-pattern>/summary</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>probedetails</servlet-name>
        <url-pattern>/details</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>whichlibs</servlet-name>
        <url-pattern>/whichlibs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>status</servlet-name>
        <url-pattern>/status</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>discover</servlet-name>
        <url-pattern>/discover</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JSonTree</servlet-name>
        <url-pattern>/jsontree</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>JSonGraph</servlet-name>
        <url-pattern>/jsongraph</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JSonPeriod</servlet-name>
        <url-pattern>/jsonperiod</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JSonQueryParams</servlet-name>
        <url-pattern>/queryparams</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cmd</servlet-name>
        <url-pattern>/cmd/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JSonPack</servlet-name>
        <url-pattern>/jsonpack/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetDiscoverHtmlCode</servlet-name>
        <url-pattern>/discoverhtml</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WhoAmI</servlet-name>
        <url-pattern>/whoami</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetGraphDesc</servlet-name>
        <url-pattern>/graphdesc</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <session-config>
        <tracking-mode>COOKIE</tracking-mode>
    </session-config>
</web-app>
