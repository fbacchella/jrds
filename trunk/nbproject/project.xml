<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>jrds.jetty</name>
            <properties/>
            <folders>
                <source-folder>
                    <label>web</label>
                    <type>doc_root</type>
                    <location>web</location>
                </source-folder>
                <source-folder>
                    <label>WEB-INF</label>
                    <type>web_inf</type>
                    <location>web/WEB-INF</location>
                </source-folder>
                <source-folder>
                    <label>src</label>
                    <type>java</type>
                    <location>src</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>war</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <target>javadoc</target>
                </action>
                <action name="test">
                    <target>test</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>war</target>
                </action>
                <action name="run">
                    <target>war</target>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>build/classes</location>
                <build-target>war</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="tree">
                        <label>Web Pages</label>
                        <location>web</location>
                    </source-folder>
                    <source-folder style="tree">
                        <label>WEB-INF Content</label>
                        <location>web/WEB-INF</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>src</label>
                        <location>src</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="run"/>
                    <ide-action name="test"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>src</package-root>
                <classpath mode="compile">../../Tools/glassfish/lib/javaee.jar:lib/activation.jar:lib/classworlds.jar:lib/geronimo-spec-jta.jar:lib/jetty-util.jar:lib/jetty.jar:lib/jstl.jar:lib/junit.jar:lib/log4j.jar:lib/mail.jar:lib/mysql-connector-java.jar:lib/plexus-container-default.jar:lib/plexus-utils.jar:lib/rrd4j-2.0.6.jar:lib/servlet-api-2.5.jar:lib/servlet-api.jar:lib/snmp4j.jar:lib/standard.jar:lib/wagon-provider-api.jar:lib/xercesImpl.jar:lib/xml-apis.jar:lib/xml-resolver.jar:lib/activation.jar:lib/jstl.jar:lib/log4j.jar:lib/mail.jar:lib/mysql-connector-java.jar:lib/servlet-api.jar:lib/snmp4j.jar:lib/standard.jar:lib/winstone.jar:lib/xercesImpl.jar:lib/xml-apis.jar:lib/xml-resolver.jar</classpath>
                <built-to>build/classes</built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
        <preferences xmlns="http://www.netbeans.org/ns/auxiliary-configuration-preferences/1">
            <module name="org-netbeans-modules-editor-indent">
                <node name="text">
                    <node name="x-java">
                        <node name="CodeStyle">
                            <node name="project"/>
                        </node>
                    </node>
                </node>
                <node name="CodeStyle">
                    <node name="project"/>
                    <property name="usedProfile" value="default"/>
                </node>
            </module>
        </preferences>
        <web-data xmlns="http://www.netbeans.org/ns/freeform-project-web/1">
            <web-module>
                <doc-root>web</doc-root>
                <classpath/>
                <context-path>/jrds</context-path>
                <j2ee-spec-level>1.5</j2ee-spec-level>
            </web-module>
        </web-data>
    </configuration>
</project>
