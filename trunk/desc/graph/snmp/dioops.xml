<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
	"urn:jrds:graphdesc">
<graphdesc>
	<name>DiskIoGraphReq</name>
	<graphName>op-{2}</graphName>
	<graphTitle>I/O operations on disk {2} on {1}</graphTitle>
	<verticalLabel>operations/s</verticalLabel>
	<add>
		<name>diskIOWrites</name>
		<graphType>line</graphType>
		<legend>Number of write accesses</legend>
	</add>
	<add>
		<name>diskIOReads</name>
		<reversed>true</reversed>
		<graphType>line</graphType>
		<legend>Number of read accesses</legend>
	</add>
	<hosttree>
		<pathelement>HOST</pathelement>
		<pathelement>DISK</pathelement>
		<pathelement>DISKACTIVITY</pathelement>
		<pathelement>INDEX</pathelement>
		<pathstring>Activity as operation/s</pathstring>
	</hosttree>
	<viewtree>
		<pathelement>DISK</pathelement>
		<pathelement>DISKACTIVITY</pathelement>
		<pathstring>Activity as operation/s</pathstring>
		<pathelement>HOST</pathelement>
		<pathelement>INDEX</pathelement>
	</viewtree>
</graphdesc>