<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
                           "urn:jrds:graphdesc">
<graphdesc>
  <name>ProcessInfoSize</name>
  <graphName>pssize-{2}</graphName>
  <graphTitle>Memory usage for process {2} on {1}</graphTitle>
  <unit>
    <binary/>
  </unit>
  <verticalLabel>Bytes</verticalLabel>
  <add>
    <name>Minimum</name>
    <graphType>line</graphType>
    <color>BLUE</color>
    <legend>Minimum</legend>
  </add>
  <add>
    <name>Maximum</name>
    <graphType>line</graphType>
    <color>GREEN</color>
    <legend>Maximum</legend>
  </add>
  <add>
    <name>Average</name>
    <graphType>line</graphType>
    <color>RED</color>
    <legend>Average</legend>
  </add>
  <hosttree>
    <pathelement>HOST</pathelement>
    <pathelement>SYSTEM</pathelement>
    <pathstring>Processus</pathstring>
    <pathelement>INDEX</pathelement>
    <pathstring>Memory usage</pathstring>
  </hosttree>
  <viewtree>
    <pathelement>SYSTEM</pathelement>
    <pathstring>Processus</pathstring>
    <pathelement>HOST</pathelement>
    <pathelement>INDEX</pathelement>
    <pathstring>Memory usage</pathstring>
  </viewtree>
</graphdesc>
