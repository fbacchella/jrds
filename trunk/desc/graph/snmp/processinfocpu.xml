<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
                           "urn:jrds:graphdesc">
<graphdesc>
  <name>ProcessInfoCpu</name>
  <graphName>pscpu-{2}</graphName>
  <graphTitle>Process usage for process {2} on {1}</graphTitle>
  <verticalLabel>%</verticalLabel>
  <add>
    <name>Cpu</name>
  </add>
  <add>
    <name>Cpupercent</name>
    <rpn>Cpu, 100, *</rpn>
    <graphType>line</graphType>
    <color>BLUE</color>
    <legend>CPU Usage</legend>
  </add>
  <hosttree>
    <pathelement>HOST</pathelement>
    <pathelement>SYSTEM</pathelement>
    <pathstring>Processus</pathstring>
    <pathelement>INDEX</pathelement>
    <pathstring>CPU usage</pathstring>
  </hosttree>
  <viewtree>
    <pathelement>SYSTEM</pathelement>
    <pathstring>Processus</pathstring>
    <pathelement>HOST</pathelement>
    <pathelement>INDEX</pathelement>
    <pathstring>CPU usage</pathstring>
  </viewtree>
</graphdesc>
