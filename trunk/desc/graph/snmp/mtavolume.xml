<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
  <name>mtaVolume</name>
  <graphName>mtavolume-{2}</graphName>
  <graphTitle>Mail traffic in bytes on {2} on {1}</graphTitle>
  <verticalLabel>bytes/s</verticalLabel>
  <lowerLimit>NaN</lowerLimit>
  <add>
    <name>mtaTransmittedVolume</name>
     <graphType>area</graphType>
    <legend>Transmitted</legend>
  </add>
  <add>
    <name>mtaReceivedVolume</name>
    <reversed>true</reversed>
    <graphType>area</graphType>
    <legend>Received</legend>
  </add>
  <hosttree>
    <pathelement>HOST</pathelement>
    <pathelement>SERVICES</pathelement>
    <pathstring>Mail</pathstring>
    <pathelement>INDEX</pathelement>
    <pathstring>Exchanged volume</pathstring>
  </hosttree>
  <viewtree>
   <pathelement>SERVICES</pathelement>
    <pathstring>Mail</pathstring>
    <pathelement>HOST</pathelement>
    <pathelement>INDEX</pathelement>
    <pathstring>Exchanged volume</pathstring>
   </viewtree>
</graphdesc>
