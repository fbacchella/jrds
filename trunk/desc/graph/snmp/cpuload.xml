<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
	"urn:jrds:graphdesc">
<graphdesc>
	<name>cpuload</name>
	<graphName>laverage</graphName>
	<graphTitle>CPU Load on ${host}</graphTitle>
	<verticalLabel>queue size</verticalLabel>
	<add>
		<name>la1</name>
		<graphType>line</graphType>
		<color>green</color>
		<legend>1mn</legend>
	</add>
	<add>
		<name>la5</name>
		<graphType>line</graphType>
		<color>blue</color>
		<legend>5mn</legend>
	</add>
	<add>
		<name>la15</name>
		<graphType>line</graphType>
		<color>red</color>
		<legend>15mn</legend>
	</add>
	<hosttree>
		<pathelement>HOST</pathelement>
		<pathelement>SYSTEM</pathelement>
		<pathelement>LOAD</pathelement>
		<pathstring>CPU Load</pathstring>
	</hosttree>
	<viewtree>
		<pathelement>SYSTEM</pathelement>
		<pathelement>LOAD</pathelement>
		<pathstring>CPU Load</pathstring>
		<pathelement>HOST</pathelement>
	</viewtree>
</graphdesc>