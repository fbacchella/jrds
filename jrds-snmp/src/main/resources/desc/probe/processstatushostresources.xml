<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>ProcessStatusHostResources</name>
    <probeName>hrpslist</probeName>
    <probeClass>jrds.probe.snmp.ProcessStatusHostResources</probeClass>
    <specific name="requester">tabular</specific>
    <ds>
        <oid>hrSWRunStatus</oid>
    </ds>
    <ds>
        <dsName>running</dsName>
        <dsType>gauge</dsType>
        <collect />
    </ds>
    <ds>
        <dsName>runnable</dsName>
        <dsType>gauge</dsType>
        <collect />
    </ds>
    <ds>
        <dsName>notRunnable</dsName>
        <dsType>gauge</dsType>
        <collect />
    </ds>
    <ds>
        <dsName>invalid</dsName>
        <dsType>gauge</dsType>
        <collect />
    </ds>
    <graphs>
        <name>processstatushostsresources</name>
    </graphs>
</probedesc>
